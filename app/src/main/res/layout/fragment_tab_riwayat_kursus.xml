<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Views.Fragments.TabRiwayatKursus">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_berlangsung_kursus"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="24dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="8dp"
                app:cardElevation="4dp"
                app:cardPreventCornerOverlap="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cons_cv_berlangsung_kursus"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/txt_tutor_berlangsung_kursus"
                        style="@style/D_OrphanTextView.Title"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:text="Intan Puji"
                        android:textColor="@color/black"
                        app:layout_constraintEnd_toStartOf="@+id/divider_card_berlangsung_kursus"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <View
                        android:id="@+id/divider_card_berlangsung_kursus"
                        android:layout_width="7dp"
                        android:layout_height="0dp"
                        android:background="@color/red500"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/txt_tarif_berlangsung_kursus"
                        style="@style/D_OrphanTextView.Body"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:layout_marginBottom="16dp"
                        android:text="Ditolak"
                        android:textColor="@color/red500"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/divider_card_berlangsung_kursus"
                        app:layout_constraintStart_toStartOf="@+id/txt_kategori_berlangsung_kursus"
                        app:layout_constraintTop_toBottomOf="@+id/txt_kategori_berlangsung_kursus" />

                    <TextView
                        android:id="@+id/txt_kategori_berlangsung_kursus"
                        style="@style/D_OrphanTextView.Body"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/background_label"
                        android:padding="6dp"
                        android:text="Catur"
                        android:textColor="@color/blue500"
                        app:layout_constraintStart_toStartOf="@+id/txt_tutor_berlangsung_kursus"
                        app:layout_constraintTop_toBottomOf="@+id/txt_tutor_berlangsung_kursus" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_berlangsung_kursus2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="24dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="8dp"
                app:cardElevation="4dp"
                app:cardPreventCornerOverlap="true"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cv_berlangsung_kursus">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cons_cv_berlangsung_kursus2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/txt_tutor_berlangsung_kursus2"
                        style="@style/D_OrphanTextView.Title"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:text="Diana"
                        android:textColor="@color/black"
                        app:layout_constraintEnd_toStartOf="@+id/divider2_card_berlangsung_kursus"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <View
                        android:id="@+id/divider2_card_berlangsung_kursus"
                        android:layout_width="7dp"
                        android:layout_height="0dp"
                        android:background="@color/green500"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/txt_tarif_berlangsung_kursus2"
                        style="@style/D_OrphanTextView.Body"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:layout_marginBottom="16dp"
                        android:text="Diakhiri"
                        android:textColor="@color/green500"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/divider2_card_berlangsung_kursus"
                        app:layout_constraintStart_toStartOf="@+id/txt_kategori_berlangsung_kursus2"
                        app:layout_constraintTop_toBottomOf="@+id/txt_kategori_berlangsung_kursus2" />

                    <TextView
                        android:id="@+id/txt_kategori_berlangsung_kursus2"
                        style="@style/D_OrphanTextView.Body"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/background_label"
                        android:padding="6dp"
                        android:text="Atletik"
                        android:textColor="@color/blue500"
                        app:layout_constraintStart_toStartOf="@+id/txt_tutor_berlangsung_kursus2"
                        app:layout_constraintTop_toBottomOf="@+id/txt_tutor_berlangsung_kursus2" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_berlangsung_kursus3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="24dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="24dp"
                android:layout_marginBottom="24dp"
                app:cardBackgroundColor="@color/white"
                app:cardCornerRadius="8dp"
                app:cardElevation="4dp"
                app:cardPreventCornerOverlap="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cv_berlangsung_kursus2">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/cons_cv_berlangsung_kursus3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/txt_tutor_berlangsung_kursus3"
                        style="@style/D_OrphanTextView.Title"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="16dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:text="Angga Hadi"
                        android:textColor="@color/black"
                        app:layout_constraintEnd_toStartOf="@+id/divider3_card_berlangsung_kursus"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <View
                        android:id="@+id/divider3_card_berlangsung_kursus"
                        android:layout_width="7dp"
                        android:layout_height="0dp"
                        android:background="@color/green500"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/txt_tarif_berlangsung_kursus3"
                        style="@style/D_OrphanTextView.Body"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:layout_marginEnd="16dp"
                        android:layout_marginBottom="16dp"
                        android:text="Diakhiri"
                        android:textColor="@color/green500"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/divider3_card_berlangsung_kursus"
                        app:layout_constraintStart_toStartOf="@+id/txt_kategori_berlangsung_kursus3"
                        app:layout_constraintTop_toBottomOf="@+id/txt_kategori_berlangsung_kursus3" />

                    <TextView
                        android:id="@+id/txt_kategori_berlangsung_kursus3"
                        style="@style/D_OrphanTextView.Body"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="16dp"
                        android:background="@drawable/background_label"
                        android:padding="6dp"
                        android:text="Karate"
                        android:textColor="@color/blue500"
                        app:layout_constraintStart_toStartOf="@+id/txt_tutor_berlangsung_kursus3"
                        app:layout_constraintTop_toBottomOf="@+id/txt_tutor_berlangsung_kursus3" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>